; Simple example for datatypes
(declare-datatype 
  TestDatatypeA
  (
   (TestConstructorA)
   (TestConstructorB (testSelectorB_0 Int))
   (TestConstructorC (testSelectorB_0 Int))
  )
)

(declare-datatypes
  (
   (TestDatatypeB 0)
   (TestDatatypeC 0)
  )
  (
    (
      (TestConstructorA)
      (TestConstructorB (testSelectorB_0 Int))
      (TestConstructorC (testSelectorC_0 Int) (testSelectorC_0 Int))
    )
    (
      (TestConstructorA)
      (TestConstructorB (testSelectorB_0 Int))
      (TestConstructorC (testSelectorB_0 Int) (testSelectorC_1 Bool))
    )
  )
)
