; Simple example for abstractions 
(declare-abstractions 
  (
   (TestAbstraction 0)
  )
  (
   (
    (TestConstructorA)
    (TestConstructorB (testSelectorB_0 Int))
    (TestConstructorC (testSelectorB_0 Int))
   )
  )
)

; Two abstractions in one declaration
(declare-abstractions
  (
   (TestAbstractionB 0)
   (TestAbstractionC 0)
  )
  (
    (
      (TestConstructorA)
      (TestConstructorB (testSelectorB_0 Int))
      (TestConstructorC (testSelectorC_0 Int) (testSelectorC_0 Int))
    )
    (
      (TestConstructorA)
      (TestConstructorB (testSelectorB_0 Int))
      (TestConstructorC (testSelectorB_0 Int) (testSelectorC_1 Bool))
    )
  )
)
